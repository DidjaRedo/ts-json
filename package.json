{
  "name": "@fgv/ts-json",
  "version": "1.9.7",
  "description": "Typescript utilities for working with JSON",
  "main": "dist/index.js",
  "scripts": {
    "prepublishOnly": "echo 'Do not publish from this folder'; exit 1",
    "build": "rimraf dist && tsc && ./prep.sh",
    "clean": "rimraf dist",
    "test": "jest",
    "test-handles": "jest --runInBand --detectOpenHandles",
    "clean-jest": "jest --clear-cache",
    "coverage": "jest --coverage --no-cache",
    "lint": "eslint src test/unit --ext .ts",
    "fixlint": "eslint src test/unit --ext .ts --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DidjaRedo/ts-json.git"
  },
  "keywords": [
    "typescript",
    "json"
  ],
  "author": "Erik Fortune",
  "license": "MIT",
  "devDependencies": {
    "@fgv/ts-utils": "^1.9.6",
    "@fgv/ts-utils-jest": "^1.9.7",
    "@types/jest": "^29.5.3",
    "@types/mustache": "^4.2.2",
    "@types/node": "^20.4.5",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint": "^8.46.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.6.2",
    "jest-extended": "^4.0.0",
    "mustache": "^4.2.0",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "@fgv/ts-utils": "^1.9.3",
    "mustache": "^4.2.0"
  }
}