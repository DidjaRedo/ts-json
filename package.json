{
  "name": "@fgv/ts-json",
  "version": "1.9.0",
  "description": "Typescript utilities for working with JSON",
  "main": "dist/index.js",
  "scripts": {
    "prepublishOnly": "echo 'Do not publish from this folder'; exit 1",
    "build": "rimraf dist && tsc && ./prep.sh",
    "clean": "rimraf dist",
    "test": "jest",
    "test-handles": "jest --runInBand --detectOpenHandles",
    "clean-jest": "jest --clear-cache",
    "coverage": "jest --coverage --no-cache",
    "lint": "eslint src test/unit --ext .ts",
    "fixlint": "eslint src test/unit --ext .ts --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DidjaRedo/ts-json.git"
  },
  "keywords": [
    "typescript",
    "json"
  ],
  "author": "Erik Fortune",
  "license": "MIT",
  "devDependencies": {
    "@fgv/ts-utils": "1.9.0",
    "@fgv/ts-utils-jest": "^1.9.0",
    "@types/jest": "^29.2.5",
    "@types/mustache": "^4.1.0",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint": "^8.31.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.3.1",
    "jest-extended": "^3.2.1",
    "rimraf": "^4.1.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "@fgv/ts-utils": "^1.9.0"
  }
}
